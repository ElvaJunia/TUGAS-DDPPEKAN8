#Tugas1 Menampilkan Data Diri

def profil(nama, alamat, gender, umur, hobby):
    print("Nama saya", nama)
    print("Alamat saya di", alamat)
    print("Gender saya", gender)
    print("Umur saya", umur)
    print("Hobby saya", hobby)

#Pemanggilan
profil("Elva Junia Sahrani", "Kp. Kebon Kelapa", "Perempuan", "18 Tahun", "Menggambar")

#Tugas2 Fungsi Untuk Mencari Kelulusan Nilai

#Nilai Kelulusan
def cek_kelulusan(nilai):
  if nilai < 60:
    return "Gagal"
  elif nilai >= 60 and nilai <= 70:
    return "Baik"
  elif nilai >= 71 and nilai <= 80:
    return "Sangat baik"
  elif nilai >= 81 and nilai <= 100:
    return "Istimewa"
  else:
    return "Nilai Tidak Ada"

#Pemanggilan
nilai(60)

#Tugas3 Fungsi Untuk Mencetak Nilai Bilangan Ganjil

def ganjil(n):
    for i in range(1, n+1, 2):
        print (i)
ganjil(100)
